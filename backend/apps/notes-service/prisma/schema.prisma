generator client {
  provider = "prisma-client-js"
  output   = "../../../node_modules/@prisma/client-notes-service"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Note {
  id        String   @id @default(uuid())
  title     String
  content   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  userId    String
}
